{
  "name": "high-roller-club-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:production": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "start:production": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/lib/**/*.test.ts",
    "test:integration": "vitest run src/components/**/*.integration.test.tsx",
    "test:visual": "vitest run src/test/visual-regression/*.test.tsx",
    "test:performance": "vitest run src/test/performance/*.test.ts",
    "test:comprehensive": "tsx src/test/test-runner.ts",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "prebuild": "npm run lint && npm run type-check",
    "postbuild": "npm run build:analyze",
    "deploy:staging": "npm run build && npm run start",
    "deploy:production": "npm run build:production && npm run start:production",
    "optimize:images": "node scripts/optimize-assets.js",
    "validate:build": "node scripts/validate-build.js"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.2",
    "@react-three/fiber": "^9.3.0",
    "critters": "^0.0.23",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.179.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
