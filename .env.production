# Production Environment Configuration
NODE_ENV=production

# Performance Optimization
NEXT_PUBLIC_PERFORMANCE_MONITORING=true
NEXT_PUBLIC_BUNDLE_ANALYZER=false
NEXT_PUBLIC_SOURCE_MAPS=false

# CDN Configuration
NEXT_PUBLIC_CDN_URL=https://cdn.highrollerclub.com
NEXT_PUBLIC_ASSET_PREFIX=https://cdn.highrollerclub.com

# Analytics Configuration (Production)
NEXT_PUBLIC_ANALYTICS_ENABLED=true
NEXT_PUBLIC_ANALYTICS_SAMPLING_RATE=0.1
NEXT_PUBLIC_ANALYTICS_EVENTS_ENDPOINT=/api/analytics/events
NEXT_PUBLIC_ANALYTICS_PERFORMANCE_ENDPOINT=/api/analytics/performance
NEXT_PUBLIC_ANALYTICS_CONVERSIONS_ENDPOINT=/api/analytics/conversions

# Security Settings
NEXT_PUBLIC_CSP_ENABLED=true
NEXT_PUBLIC_HSTS_ENABLED=true

# 3D Rendering Optimization (Production)
NEXT_PUBLIC_ANTIALIASING=true
NEXT_PUBLIC_SHADOW_MAP_SIZE=2048
NEXT_PUBLIC_MAX_LIGHTS=6
NEXT_PUBLIC_LOD_ENABLED=true
NEXT_PUBLIC_TARGET_FPS=60

# Asset Optimization
NEXT_PUBLIC_ASSET_COMPRESSION=true
NEXT_PUBLIC_ASSET_OPTIMIZATION=true
NEXT_PUBLIC_PRELOAD_CRITICAL_ASSETS=true

# Cache Configuration
NEXT_PUBLIC_CACHE_MAX_AGE=31536000
NEXT_PUBLIC_STATIC_CACHE_MAX_AGE=86400

# Feature Flags (Production)
NEXT_PUBLIC_EXPERIMENTAL_FEATURES=false
NEXT_PUBLIC_BETA_FEATURES=false
NEXT_PUBLIC_DEBUG_MODE=false

# Discord Configuration
NEXT_PUBLIC_DISCORD_SERVER_INVITE=https://discord.gg/RK4K8tDsHB
